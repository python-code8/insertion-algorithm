def insertion_sort(l):
    for i in range(1, len(l)):
        j = i-1 
        next_element = l[i]

# Compare the current element with next one
        while (l[j] > next_element) and (j >=0):
            l[j+1] = l[j]
            j=j-1
        l[j+1] next_element

list = [19,2,31,45,30,11,121,27]
insertion_sort(list)
print(list)
